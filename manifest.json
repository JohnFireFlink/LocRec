{
  "manifest_version": 3,
  "name": "Locator Capture Tool",
  "version": "1.0",
  "description": "Capture web element locators by clicking on elements.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
  }